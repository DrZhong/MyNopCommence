@model IList<ProductOverviewModel>

@using Nop.Web.Models.Catalog;

@if (Model.Count > 0)
{
    <div class="index-layout-one  fn-clear">
        <div class="title">
            <span class="name"><i>&nbsp;</i>@T("HomePage.Products")</span>
            <span class="more"><a href="/样片拍摄" title="样片拍摄">More+</a> </span>
        </div>
        <div class="product-grid">
            @foreach (var item in Model)
            {
                <div class="item-box">
                    @Html.Partial("_ProductBox", item)
                </div>
            }
        </div>
    </div>
}